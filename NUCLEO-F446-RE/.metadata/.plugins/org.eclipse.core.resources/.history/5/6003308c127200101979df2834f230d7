#include "stm32f4xx.h"

int main(void)
{
	RCC->AHB1ENR |= RCC_AHB1ENR_GPIOAEN;

	GPIOA->MODER &= ~(GPIO_MODER_MODER5_Msk);
	GPIOA->MODER |= GPIO_MODER_MODER5_0;

	while(1)
	{
		GPIOA->BSRR = ( 1<<5);

		for(volatile int i = 0; i<500000; i++)
		{

		}

		GPIOA->BSRR = ( 1<<(5+16));

		for (volatile int i = 0; i< 500000; i++)
		{

		}

	}

}
