#include <stdint.h>



#define RCC_BASE       0x40023800




typedef struct
{
	volatile uint32_t RCC_CR;
	volatile uint32_t PLLCFGR;
	volatile uint32_t CFGR;
	CIR;
	AHB1RSTR;
	AHB2RSTR;
	AHB3RSTR;
	RESERVED0;
	APB1RSTR;
	APB2RSTR;
	RESERVED1[2];
	AHB1ENR;
	AHB2ENR;
	AHB3ENR;
	RESERVED2;
	APB1ENR;
	APB2ENR;
	RESERVED3[2];
	AHB1LPENR;
	AHB2LPENR;
	AHB3LPENR;
	RESERVED4;
	APB1LPENR;
	APB2LPENR;
	RESERVED4[2];
	BDCR;
	CSR;
	RESERVED5[2];
	SSCGR;
	PLLI2SCFGR;
	PLLSAICFGR;
	DCKCFGR;
	CKGATENR;
	DCKCFGR2;
}RCC_Typedef;
